	pmcid = {PMC6927249},
	keywords = {Amino Acid Sequence, CASP, community wide experiment, Computational Biology, Humans, Models, Molecular, Protein Conformation, protein structure prediction, Proteins, Scattering, Small Angle, X-Ray Diffraction},
	pages = {1011--1020},
	file = {Accepted Version:/home/hmacdope/Zotero/storage/TVAMLWUR/Kryshtafovych et al. - 2019 - Critical assessment of methods of protein structure prediction (CASP)-Round XIII.pdf:application/pdf},
}



